{
  "name": "text2speech",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch -c development",
    "t": "jest app.component.spec.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "build:staging": "ng build -c staging",
    "build:production": "ng build -c production",
    "build:stats": "ng build -c staging --stats-json ",
    "analyze": "webpack-bundle-analyzer dist/text2speech/stats.json",
    "dev": "ng s --host=0.0.0.0 --disable-host-check",
    "stag": "ng s -c staging --host=0.0.0.0 --disable-host-check",
    "prod": "ng s -c prod_test --host=0.0.0.0 --disable-host-check",
    "b": "npm run build:staging",
    "o": "npm run build:stats && npm run analyze"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.1",
    "@angular/cdk": "^17.2.0",
    "@angular/common": "^17.2.1",
    "@angular/compiler": "^17.2.1",
    "@angular/core": "^17.2.1",
    "@angular/forms": "^17.2.1",
    "@angular/material": "^17.2.0",
    "@angular/platform-browser": "^17.2.1",
    "@angular/platform-browser-dynamic": "^17.2.1",
    "@angular/router": "^17.2.1",
    "@angular/service-worker": "^17.2.1",
    "@firebase/app": "^0.9.22",
    "@firebase/auth": "^1.3.2",
    "@line/liff": "^2.22.4",
    "angularx-qrcode": "^17.0.0",
    "fast-levenshtein": "^3.0.0",
    "jszip": "^3.10.1",
    "jwt-decode": "^4.0.0",
    "ngx-cookie-service": "^17.1.0",
    "ngx-infinite-scroll": "^17.0.0",
    "ngx-loading": "^17.0.0",
    "ngx-skeleton-loader": "^6.0.0",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.9.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.0",
    "@angular/cli": "~17.2.0",
    "@angular/compiler-cli": "^17.2.1",
    "@types/jest": "^29.5.12",
    "@types/recordrtc": "^5.6.13",
    "cookie-parser": "^1.4.6",
    "file-saver": "^2.0.5",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.3",
    "ng-mocks": "^14.12.1",
    "prettier": "^3.2.5",
    "typescript": "~5.3.3",
    "webpack-bundle-analyzer": "^4.9.1"
  }
}
